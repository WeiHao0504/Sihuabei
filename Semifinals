                                            document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second2_1+':'+N_minute2_1+'</h1>';
                                        }else if(N_minute2_1<=9){
                                            document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                        }

                                    }else if (N_minute2_1>9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+'0'+N_second2_1+':'+N_minute2_1+'</h1>';
                                    }else if(N_minute2_1<=9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+'0'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 10px;">'+second2_1+':'+minute2_1+'</h1>';
                            }
                        }

                        function N_countdown2_1()
                        {
                            // if (timeout2_1 < 0)
                            // {
                            //     document.getElementById("timedown2_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout2_1 >=0)
                            {

                                if (N_timeout2_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout2_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second2_1>9){
                                    if (N_minute2_1>9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+N_second2_1+':'+N_minute2_1+'</h1>';
                                    }else if(N_minute2_1<=9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                    }
                                }else if(N_second2_1<=9){

                                    if (N_second2_1==0&&N_minute2_1<=30){

                                        if (N_minute2_1>9){
                                            document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second2_1+':'+N_minute2_1+'</h1>';
                                        }else if(N_minute2_1<=9){
                                            document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                        }

                                    }else if (N_minute2_1>9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+'0'+N_second2_1+':'+N_minute2_1+'</h1>';
                                    }else if(N_minute2_1<=9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+'0'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 10px;">'+second2_1+':'+minute2_1+'</h1>';
                                N_handle2_1 = setTimeout("N_countdown2_1()",1000);
                            }
                            N_timeout2_1--;
                            if (N_minute2_1>0&&N_minute2_1<=60){
                                N_minute2_1--;
                            }else if (N_minute2_1==0&&N_second2_1!=0){
                                N_minute2_1 = 59;
                                N_second2_1--;

                            }

                        }
                        // countdown2_1();T

                        function N_stoptime2_1(){
                            clearTimeout(N_handle2_1);
                            N_flag2_1 = 0;
                        }

                    </script>

                









        


    

