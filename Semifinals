                            {
                                if (timeout5_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout5_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second5_1>9){
                                    if (minute5_1>9){
                                        document.getElementById("timedown5_1").innerHTML='<h1 id="timedown5_1" style="margin-top: 18px;">'+second5_1+':'+minute5_1+'</h1>';
