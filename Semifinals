                                <button id="N_beginBtn4_1" class="btn btn-primary btn-sm" style="background-color: #306A96" onclick="N_timercontroller4_1()" >开始
                                </button>
                                <button id="N_stopBtn4_1" class="btn btn-danger btn-sm" style="background-color: #AA3131" onclick="N_stoptime4_1()">停止</button>
                                <!--<button id="resetBtn6_1" class="btn btn-warning btn-sm" style="background-color: #ED972F">重置-->
                                <!--</button>-->
                            </div>

                        </div>
                    </div>
                </div>
                <!-- 结束 -->

            

            

            
            
            
        





