        <!-- 计时器数据渲染以及控制-->

                

                    <script>



                        var timeout4_1='180';

                        var second4_1 = parseInt(timeout4_1/60);
                        var minute4_1 = timeout4_1%60;
                        var handle4_1;
                        var flag4_1;

                        timerinit4_1();

                        function timercontroller4_1() {
                            if (flag4_1==0){
                                countdown4_1();
                                flag4_1 = 1;
                            }
                        }


                        function timerinit4_1() {

                            flag4_1 = 0;

                            if (timeout4_1 >=0)
                            {
                                if (second4_1>9){
                                    if (minute4_1>9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+second4_1+':'+minute4_1+'</h1>';
                                    }else if(minute4_1<=9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+second4_1+':'+'0'+minute4_1+'</h1>';
                                    }
                                }else if(second4_1<=9){

                                    if (second4_1==0&&minute4_1<=30){

                                        if (minute4_1>9){
                                            document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+second4_1+':'+minute4_1+'</h1>';
                                        }else if(minute4_1<=9){
                                            document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+second4_1+':'+'0'+minute4_1+'</h1>';
                                        }

                                    }else if (minute4_1>9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+'0'+second4_1+':'+minute4_1+'</h1>';
                                    }else if(minute4_1<=9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+'0'+second4_1+':'+'0'+minute4_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 10px;">'+second4_1+':'+minute4_1+'</h1>';
                            }
                        }

                        function countdown4_1()
                        {
                            // if (timeout4_1 < 0)
                            // {
                            //     document.getElementById("timedown4_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout4_1 >=0)
                            {
                                if (timeout4_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout4_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second4_1>9){
                                    if (minute4_1>9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+second4_1+':'+minute4_1+'</h1>';
                                    }else if(minute4_1<=9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+second4_1+':'+'0'+minute4_1+'</h1>';
                                    }
                                }else if(second4_1<=9){

                                    if (second4_1==0&&minute4_1<=30){

                                        if (minute4_1>9){
                                            document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+second4_1+':'+minute4_1+'</h1>';
                                        }else if(minute4_1<=9){
                                            document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+second4_1+':'+'0'+minute4_1+'</h1>';
                                        }

                                    }else if (minute4_1>9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+'0'+second4_1+':'+minute4_1+'</h1>';
                                    }else if(minute4_1<=9){
                                        document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 18px;">'+'0'+second4_1+':'+'0'+minute4_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 10px;">'+second4_1+':'+minute4_1+'</h1>';
                                handle4_1 = setTimeout("countdown4_1()",1000);
                            }
                            timeout4_1--;
                            if (minute4_1>0&&minute4_1<=60){

                                minute4_1--;
                            }else if (minute4_1==0&&second4_1!=0){


                                minute4_1 = 59;
                                second4_1--;

                            }

                        }
                        // countdown4_1();T

                        function stoptime4_1(){
                            clearTimeout(handle4_1);
                            flag4_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout4_1='180';

                        var N_second4_1 = parseInt(N_timeout4_1/60);
                        var N_minute4_1 = N_timeout4_1%60;
                        var N_handle4_1;
                        var N_flag4_1;

                        N_timerinit4_1();

                        function N_timercontroller4_1() {
                            if (N_flag4_1==0){
                                N_countdown4_1();
                                N_flag4_1 = 1;
                            }
                        }


                        function N_timerinit4_1() {

                            N_flag4_1 = 0;

                            if (N_timeout4_1 >=0)
                            {
                                if (N_second4_1>9){
                                    if (N_minute4_1>9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+N_second4_1+':'+N_minute4_1+'</h1>';
                                    }else if(N_minute4_1<=9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                    }
                                }else if(N_second4_1<=9){

                                    if (N_second4_1==0&&N_minute4_1<=30){

                                        if (N_minute4_1>9){
                                            document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second4_1+':'+N_minute4_1+'</h1>';
                                        }else if(N_minute4_1<=9){
                                            document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                        }

                                    }else if (N_minute4_1>9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+'0'+N_second4_1+':'+N_minute4_1+'</h1>';
                                    }else if(N_minute4_1<=9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+'0'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 10px;">'+second4_1+':'+minute4_1+'</h1>';
                            }
                        }

                        function N_countdown4_1()
                        {
                            // if (timeout4_1 < 0)
                            // {
                            //     document.getElementById("timedown4_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout4_1 >=0)
                            {

                                if (N_timeout4_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout4_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second4_1>9){
                                    if (N_minute4_1>9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+N_second4_1+':'+N_minute4_1+'</h1>';
                                    }else if(N_minute4_1<=9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                    }
                                }else if(N_second4_1<=9){

                                    if (N_second4_1==0&&N_minute4_1<=30){

                                        if (N_minute4_1>9){
                                            document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second4_1+':'+N_minute4_1+'</h1>';
                                        }else if(N_minute4_1<=9){
                                            document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                        }

                                    }else if (N_minute4_1>9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+'0'+N_second4_1+':'+N_minute4_1+'</h1>';
                                    }else if(N_minute4_1<=9){
                                        document.getElementById("N_timedown4_1").innerHTML='<h1 id="N_timedown4_1" style="margin-top: 18px;">'+'0'+N_second4_1+':'+'0'+N_minute4_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown4_1").innerHTML='<h1 id="timedown4_1" style="margin-top: 10px;">'+second4_1+':'+minute4_1+'</h1>';
                                N_handle4_1 = setTimeout("N_countdown4_1()",1000);
                            }
                            N_timeout4_1--;
                            if (N_minute4_1>0&&N_minute4_1<=60){
                                N_minute4_1--;
                            }else if (N_minute4_1==0&&N_second4_1!=0){
                                N_minute4_1 = 59;
                                N_second4_1--;

                            }

                        }
                        // countdown4_1();T

                        function N_stoptime4_1(){
                            clearTimeout(N_handle4_1);
                            N_flag4_1 = 0;
                        }

                    </script>

                









        


    

