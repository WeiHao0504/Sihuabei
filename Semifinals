                        }

                        function N_countdown3_1()
                        {
                            // if (timeout3_1 < 0)
                            // {
                            //     document.getElementById("timedown3_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout3_1 >=0)
                            {

                                if (N_timeout3_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout3_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second3_1>9){
                                    if (N_minute3_1>9){
                                        document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;">'+N_second3_1+':'+N_minute3_1+'</h1>';
                                    }else if(N_minute3_1<=9){
                                        document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;">'+N_second3_1+':'+'0'+N_minute3_1+'</h1>';
                                    }
                                }else if(N_second3_1<=9){

                                    if (N_second3_1==0&&N_minute3_1<=30){

                                        if (N_minute3_1>9){
                                            document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second3_1+':'+N_minute3_1+'</h1>';
                                        }else if(N_minute3_1<=9){
                                            document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second3_1+':'+'0'+N_minute3_1+'</h1>';
                                        }

                                    }else if (N_minute3_1>9){
                                        document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;">'+'0'+N_second3_1+':'+N_minute3_1+'</h1>';
                                    }else if(N_minute3_1<=9){
                                        document.getElementById("N_timedown3_1").innerHTML='<h1 id="N_timedown3_1" style="margin-top: 18px;">'+'0'+N_second3_1+':'+'0'+N_minute3_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 10px;">'+second3_1+':'+minute3_1+'</h1>';
                                N_handle3_1 = setTimeout("N_countdown3_1()",1000);
                            }
                            N_timeout3_1--;
                            if (N_minute3_1>0&&N_minute3_1<=60){
                                N_minute3_1--;
                            }else if (N_minute3_1==0&&N_second3_1!=0){
                                N_minute3_1 = 59;
                                N_second3_1--;

                            }

                        }
