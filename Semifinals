        <!-- 计时器数据渲染以及控制-->

                

                    <script>



                        var timeout6_1='180';

                        var second6_1 = parseInt(timeout6_1/60);
                        var minute6_1 = timeout6_1%60;
                        var handle6_1;
                        var flag6_1;

                        timerinit6_1();

                        function timercontroller6_1() {
                            if (flag6_1==0){
                                countdown6_1();
                                flag6_1 = 1;
                            }
                        }


                        function timerinit6_1() {

                            flag6_1 = 0;

                            if (timeout6_1 >=0)
                            {
                                if (second6_1>9){
                                    if (minute6_1>9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+second6_1+':'+minute6_1+'</h1>';
                                    }else if(minute6_1<=9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+second6_1+':'+'0'+minute6_1+'</h1>';
                                    }
                                }else if(second6_1<=9){

                                    if (second6_1==0&&minute6_1<=30){

                                        if (minute6_1>9){
                                            document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+second6_1+':'+minute6_1+'</h1>';
                                        }else if(minute6_1<=9){
                                            document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+second6_1+':'+'0'+minute6_1+'</h1>';
                                        }

                                    }else if (minute6_1>9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+'0'+second6_1+':'+minute6_1+'</h1>';
                                    }else if(minute6_1<=9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+'0'+second6_1+':'+'0'+minute6_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 10px;">'+second6_1+':'+minute6_1+'</h1>';
                            }
                        }

                        function countdown6_1()
                        {
                            // if (timeout6_1 < 0)
                            // {
                            //     document.getElementById("timedown6_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout6_1 >=0)
                            {
                                if (timeout6_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout6_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second6_1>9){
                                    if (minute6_1>9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+second6_1+':'+minute6_1+'</h1>';
                                    }else if(minute6_1<=9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+second6_1+':'+'0'+minute6_1+'</h1>';
                                    }
                                }else if(second6_1<=9){

                                    if (second6_1==0&&minute6_1<=30){

                                        if (minute6_1>9){
                                            document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+second6_1+':'+minute6_1+'</h1>';
                                        }else if(minute6_1<=9){
                                            document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+second6_1+':'+'0'+minute6_1+'</h1>';
                                        }

                                    }else if (minute6_1>9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+'0'+second6_1+':'+minute6_1+'</h1>';
                                    }else if(minute6_1<=9){
                                        document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 18px;">'+'0'+second6_1+':'+'0'+minute6_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 10px;">'+second6_1+':'+minute6_1+'</h1>';
                                handle6_1 = setTimeout("countdown6_1()",1000);
                            }
                            timeout6_1--;
                            if (minute6_1>0&&minute6_1<=60){

                                minute6_1--;
                            }else if (minute6_1==0&&second6_1!=0){


                                minute6_1 = 59;
                                second6_1--;

                            }

                        }
                        // countdown6_1();T

                        function stoptime6_1(){
                            clearTimeout(handle6_1);
                            flag6_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout6_1='180';

                        var N_second6_1 = parseInt(N_timeout6_1/60);
                        var N_minute6_1 = N_timeout6_1%60;
                        var N_handle6_1;
                        var N_flag6_1;

                        N_timerinit6_1();

                        function N_timercontroller6_1() {
                            if (N_flag6_1==0){
                                N_countdown6_1();
                                N_flag6_1 = 1;
                            }
                        }


                        function N_timerinit6_1() {

                            N_flag6_1 = 0;

                            if (N_timeout6_1 >=0)
                            {
                                if (N_second6_1>9){
                                    if (N_minute6_1>9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+N_second6_1+':'+N_minute6_1+'</h1>';
                                    }else if(N_minute6_1<=9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                    }
                                }else if(N_second6_1<=9){

                                    if (N_second6_1==0&&N_minute6_1<=30){

                                        if (N_minute6_1>9){
                                            document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second6_1+':'+N_minute6_1+'</h1>';
                                        }else if(N_minute6_1<=9){
                                            document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                        }

                                    }else if (N_minute6_1>9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+'0'+N_second6_1+':'+N_minute6_1+'</h1>';
                                    }else if(N_minute6_1<=9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+'0'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 10px;">'+second6_1+':'+minute6_1+'</h1>';
                            }
                        }

                        function N_countdown6_1()
                        {
                            // if (timeout6_1 < 0)
                            // {
                            //     document.getElementById("timedown6_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout6_1 >=0)
                            {

                                if (N_timeout6_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout6_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second6_1>9){
                                    if (N_minute6_1>9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+N_second6_1+':'+N_minute6_1+'</h1>';
                                    }else if(N_minute6_1<=9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                    }
                                }else if(N_second6_1<=9){

                                    if (N_second6_1==0&&N_minute6_1<=30){

                                        if (N_minute6_1>9){
                                            document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second6_1+':'+N_minute6_1+'</h1>';
                                        }else if(N_minute6_1<=9){
                                            document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                        }

                                    }else if (N_minute6_1>9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+'0'+N_second6_1+':'+N_minute6_1+'</h1>';
                                    }else if(N_minute6_1<=9){
                                        document.getElementById("N_timedown6_1").innerHTML='<h1 id="N_timedown6_1" style="margin-top: 18px;">'+'0'+N_second6_1+':'+'0'+N_minute6_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown6_1").innerHTML='<h1 id="timedown6_1" style="margin-top: 10px;">'+second6_1+':'+minute6_1+'</h1>';
                                N_handle6_1 = setTimeout("N_countdown6_1()",1000);
                            }
                            N_timeout6_1--;
                            if (N_minute6_1>0&&N_minute6_1<=60){
                                N_minute6_1--;
                            }else if (N_minute6_1==0&&N_second6_1!=0){
                                N_minute6_1 = 59;
                                N_second6_1--;

                            }

                        }
                        // countdown6_1();T

                        function N_stoptime6_1(){
                            clearTimeout(N_handle6_1);
                            N_flag6_1 = 0;
                        }

                    </script>

                









        


    

