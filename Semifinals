                        function N_timercontroller9_1() {
                            if (N_flag9_1==0){
                                N_countdown9_1();
                                N_flag9_1 = 1;
                            }
                        }


                        function N_timerinit9_1() {

                            N_flag9_1 = 0;

                            if (N_timeout9_1 >=0)
                            {
                                if (N_second9_1>9){
                                    if (N_minute9_1>9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+N_second9_1+':'+N_minute9_1+'</h1>';
                                    }else if(N_minute9_1<=9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                    }
                                }else if(N_second9_1<=9){

                                    if (N_second9_1==0&&N_minute9_1<=30){

                                        if (N_minute9_1>9){
                                            document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second9_1+':'+N_minute9_1+'</h1>';
                                        }else if(N_minute9_1<=9){
                                            document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                        }

                                    }else if (N_minute9_1>9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+'0'+N_second9_1+':'+N_minute9_1+'</h1>';
                                    }else if(N_minute9_1<=9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+'0'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown9_1").innerHTML='<h1 id="timedown9_1" style="margin-top: 10px;">'+second9_1+':'+minute9_1+'</h1>';
                            }
                        }

                        function N_countdown9_1()
                        {
                            // if (timeout9_1 < 0)
                            // {
                            //     document.getElementById("timedown9_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout9_1 >=0)
                            {

                                if (N_timeout9_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout9_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second9_1>9){
                                    if (N_minute9_1>9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+N_second9_1+':'+N_minute9_1+'</h1>';
                                    }else if(N_minute9_1<=9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                    }
                                }else if(N_second9_1<=9){

                                    if (N_second9_1==0&&N_minute9_1<=30){

                                        if (N_minute9_1>9){
                                            document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second9_1+':'+N_minute9_1+'</h1>';
                                        }else if(N_minute9_1<=9){
                                            document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                        }

                                    }else if (N_minute9_1>9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+'0'+N_second9_1+':'+N_minute9_1+'</h1>';
                                    }else if(N_minute9_1<=9){
                                        document.getElementById("N_timedown9_1").innerHTML='<h1 id="N_timedown9_1" style="margin-top: 18px;">'+'0'+N_second9_1+':'+'0'+N_minute9_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown9_1").innerHTML='<h1 id="timedown9_1" style="margin-top: 10px;">'+second9_1+':'+minute9_1+'</h1>';
                                N_handle9_1 = setTimeout("N_countdown9_1()",1000);
                            }
                            N_timeout9_1--;
                            if (N_minute9_1>0&&N_minute9_1<=60){
                                N_minute9_1--;
                            }else if (N_minute9_1==0&&N_second9_1!=0){
                                N_minute9_1 = 59;
                                N_second9_1--;

                            }

                        }
                        // countdown9_1();T

                        function N_stoptime9_1(){
                            clearTimeout(N_handle9_1);
                            N_flag9_1 = 0;
                        }

                    </script>

                









        


    

