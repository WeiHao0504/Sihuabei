                            <div class="btn-group" role="group" aria-label="...">
                                <button id="beginBtn10_1" class="btn btn-primary btn-sm" style="background-color: #306A96" onclick="timercontroller10_1()" >开始</button>
                                <button id="stopBtn10_1" class="btn btn-danger btn-sm" style="background-color: #AA3131" onclick="stoptime10_1()">停止</button>
