        <!-- 计时器数据渲染以及控制-->

                

                    <script>



                        var timeout8_1='240';

                        var second8_1 = parseInt(timeout8_1/60);
                        var minute8_1 = timeout8_1%60;
                        var handle8_1;
                        var flag8_1;

                        timerinit8_1();

                        function timercontroller8_1() {
                            if (flag8_1==0){
                                countdown8_1();
                                flag8_1 = 1;
                            }
                        }


                        function timerinit8_1() {

                            flag8_1 = 0;

                            if (timeout8_1 >=0)
                            {
                                if (second8_1>9){
                                    if (minute8_1>9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+second8_1+':'+minute8_1+'</h1>';
                                    }else if(minute8_1<=9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+second8_1+':'+'0'+minute8_1+'</h1>';
                                    }
                                }else if(second8_1<=9){

                                    if (second8_1==0&&minute8_1<=30){

                                        if (minute8_1>9){
                                            document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+second8_1+':'+minute8_1+'</h1>';
                                        }else if(minute8_1<=9){
                                            document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+second8_1+':'+'0'+minute8_1+'</h1>';
                                        }

                                    }else if (minute8_1>9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+'0'+second8_1+':'+minute8_1+'</h1>';
                                    }else if(minute8_1<=9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+'0'+second8_1+':'+'0'+minute8_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 10px;">'+second8_1+':'+minute8_1+'</h1>';
                            }
                        }

                        function countdown8_1()
                        {
                            // if (timeout8_1 < 0)
                            // {
                            //     document.getElementById("timedown8_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout8_1 >=0)
                            {
                                if (timeout8_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout8_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second8_1>9){
                                    if (minute8_1>9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+second8_1+':'+minute8_1+'</h1>';
                                    }else if(minute8_1<=9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+second8_1+':'+'0'+minute8_1+'</h1>';
                                    }
                                }else if(second8_1<=9){

                                    if (second8_1==0&&minute8_1<=30){

                                        if (minute8_1>9){
                                            document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+second8_1+':'+minute8_1+'</h1>';
                                        }else if(minute8_1<=9){
                                            document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+second8_1+':'+'0'+minute8_1+'</h1>';
                                        }

                                    }else if (minute8_1>9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+'0'+second8_1+':'+minute8_1+'</h1>';
                                    }else if(minute8_1<=9){
                                        document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 18px;">'+'0'+second8_1+':'+'0'+minute8_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 10px;">'+second8_1+':'+minute8_1+'</h1>';
                                handle8_1 = setTimeout("countdown8_1()",1000);
                            }
                            timeout8_1--;
                            if (minute8_1>0&&minute8_1<=60){

                                minute8_1--;
                            }else if (minute8_1==0&&second8_1!=0){


                                minute8_1 = 59;
                                second8_1--;

                            }

                        }
                        // countdown8_1();T

                        function stoptime8_1(){
                            clearTimeout(handle8_1);
                            flag8_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout8_1='0';

                        var N_second8_1 = parseInt(N_timeout8_1/60);
                        var N_minute8_1 = N_timeout8_1%60;
                        var N_handle8_1;
                        var N_flag8_1;

                        N_timerinit8_1();

                        function N_timercontroller8_1() {
                            if (N_flag8_1==0){
                                N_countdown8_1();
                                N_flag8_1 = 1;
                            }
                        }


                        function N_timerinit8_1() {

                            N_flag8_1 = 0;

                            if (N_timeout8_1 >=0)
                            {
                                if (N_second8_1>9){
                                    if (N_minute8_1>9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+N_second8_1+':'+N_minute8_1+'</h1>';
                                    }else if(N_minute8_1<=9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                    }
                                }else if(N_second8_1<=9){

                                    if (N_second8_1==0&&N_minute8_1<=30){

                                        if (N_minute8_1>9){
                                            document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second8_1+':'+N_minute8_1+'</h1>';
                                        }else if(N_minute8_1<=9){
                                            document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                        }

                                    }else if (N_minute8_1>9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+'0'+N_second8_1+':'+N_minute8_1+'</h1>';
                                    }else if(N_minute8_1<=9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+'0'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 10px;">'+second8_1+':'+minute8_1+'</h1>';
                            }
                        }

                        function N_countdown8_1()
                        {
                            // if (timeout8_1 < 0)
                            // {
                            //     document.getElementById("timedown8_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout8_1 >=0)
                            {

                                if (N_timeout8_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout8_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second8_1>9){
                                    if (N_minute8_1>9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+N_second8_1+':'+N_minute8_1+'</h1>';
                                    }else if(N_minute8_1<=9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                    }
                                }else if(N_second8_1<=9){

                                    if (N_second8_1==0&&N_minute8_1<=30){

                                        if (N_minute8_1>9){
                                            document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second8_1+':'+N_minute8_1+'</h1>';
                                        }else if(N_minute8_1<=9){
                                            document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                        }

                                    }else if (N_minute8_1>9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+'0'+N_second8_1+':'+N_minute8_1+'</h1>';
                                    }else if(N_minute8_1<=9){
                                        document.getElementById("N_timedown8_1").innerHTML='<h1 id="N_timedown8_1" style="margin-top: 18px;">'+'0'+N_second8_1+':'+'0'+N_minute8_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown8_1").innerHTML='<h1 id="timedown8_1" style="margin-top: 10px;">'+second8_1+':'+minute8_1+'</h1>';
                                N_handle8_1 = setTimeout("N_countdown8_1()",1000);
                            }
                            N_timeout8_1--;
                            if (N_minute8_1>0&&N_minute8_1<=60){
                                N_minute8_1--;
                            }else if (N_minute8_1==0&&N_second8_1!=0){
                                N_minute8_1 = 59;
                                N_second8_1--;

                            }

                        }
                        // countdown8_1();T

                        function N_stoptime8_1(){
                            clearTimeout(N_handle8_1);
                            N_flag8_1 = 0;
                        }

                    </script>

                









        


    

