        <!-- 计时器数据渲染以及控制-->

                









        
            <script>
                //获取自由辩论双方总时间
                var Free_Atime7 ='240';
                var Free_Ntime7 = '240';

                //双方时间转换为分秒单位
                var Free_Asecond7 = parseInt(Free_Atime7/60);
                var Free_Aminute7 = Free_Atime7%60;

                var Free_Nsecond7 = parseInt(Free_Ntime7/60);
                var Free_Nminute7 = Free_Ntime7%60;

                //定义计时handle以及flag
                var Free_Ahandle7;
                var Free_Nhandle7;
                var Free_Aflag7;
                var Free_Nflag7;


                free_init7()

                function Free_Atimercontroller7() {
                    if (Free_Aflag7==0){
                        free_countdown_A7();
                        Free_Aflag7 = 1;
                    }
                }
                function Free_Ntimercontroller7() {
                    if (Free_Nflag7==0){
                        free_countdown_N7();
                        Free_Nflag7 = 1;
                    }
                }


                //定义初始化函数
                function free_init7() {
                    Free_Aflag7 = 0;
                    Free_Nflag7 = 0;

                    //正方时间初始化完成
                    if (Free_Atime7 >=0)
                    {
                        if (Free_Asecond7>9){
                            if (Free_Aminute7>9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                            }else if(Free_Aminute7<=9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                            }
                        }else if(Free_Asecond7<=9){

                            if (Free_Asecond7==0&&Free_Aminute7<=30){

                                if (Free_Aminute7>9){
                                    document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                                }else if(Free_Aminute7<=9){
                                    document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                                }

                            }else if (Free_Aminute7>9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+'0'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                            }else if(Free_Aminute7<=9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+'0'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                            }

                        }

                        // document.getElementById("timedown7_1").innerHTML='<h1 id="timedown7_1" style="margin-top: 10px;">'+second7_+':'+minute7_+'</h1>';
                    }

                    //反方时间初始化完成
                    if (Free_Ntime7 >=0)
                    {
                        if (Free_Nsecond7>9){
                            if (Free_Nminute7>9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                            }else if(Free_Nminute7<=9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                            }
                        }else if(Free_Nsecond7<=9){

                            if (Free_Nsecond7==0&&Free_Nminute7<=30){

                                if (Free_Nminute7>9){
                                    document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                                }else if(Free_Nminute7<=9){
                                    document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                                }

                            }else if (Free_Nminute7>9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+'0'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                            }else if(Free_Nminute7<=9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+'0'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                            }

                        }

                        // document.getElementById("timedown7_1").innerHTML='<h1 id="timedown7_1" style="margin-top: 10px;">'+second7_+':'+minute7_+'</h1>';
                    }



                }


                //定义计时函数
                function Free_counttime7() {
                    if(Free_Aflag7==0&&Free_Nflag7==0){
                        Free_Atimercontroller7();

                    }else if (Free_Aflag7==0&&Free_Nflag7==1){
                        Free_Nstoptime7()
                        Free_Atimercontroller7()


                    }else if (Free_Aflag7==1&&Free_Nflag7==0){
                        Free_Astoptime7()
                        Free_Ntimercontroller7()
                    }
                }

                function free_countdown_A7() {
                    if (Free_Atime7 >=0)
                    {
                        if (Free_Atime7 ==0){
                            $("#jquery_jplayer_2").jPlayer("play");
                        }
                        if(Free_Atime7 ==30){
                            $("#jquery_jplayer_1").jPlayer("play");
                        }


                        if (Free_Asecond7>9){
                            if (Free_Aminute7>9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                            }else if(Free_Aminute7<=9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                            }
                        }else if(Free_Asecond7<=9){

                            if (Free_Asecond7==0&&Free_Aminute7<=30){

                                if (Free_Aminute7>9){
                                    document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                                }else if(Free_Aminute7<=9){
                                    document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                                }

                            }else if (Free_Aminute7>9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+'0'+Free_Asecond7+':'+Free_Aminute7+'</h1>';
                            }else if(Free_Aminute7<=9){
                                document.getElementById("timedown7_free").innerHTML='<h1 id="timedown7_free" style="margin-top: 18px;">'+'0'+Free_Asecond7+':'+'0'+Free_Aminute7+'</h1>';
                            }

                        }


                        // document.getElementById("timedown7_1").innerHTML='<h1 id="timedown7_1" style="margin-top: 10px;">'+second7_+':'+minute7_+'</h1>';
                        Free_Ahandle7 = setTimeout("free_countdown_A7()",1000);
                    }
                    Free_Atime7--;
                    if (Free_Aminute7>0&&Free_Aminute7<=60){
                        Free_Aminute7--;
                    }else if (Free_Aminute7==0&&Free_Asecond7!=0){
                        Free_Aminute7 = 59;
                        Free_Asecond7--;

                    }
                }
                function free_countdown_N7() {
                    if (Free_Ntime7 >=0)
                    {

                        if (Free_Ntime7 ==0){
                            $("#jquery_jplayer_2").jPlayer("play");
                        }
                        if(Free_Ntime7 ==30){
                            $("#jquery_jplayer_1").jPlayer("play");
                        }

                        if (Free_Nsecond7>9){
                            if (Free_Nminute7>9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                            }else if(Free_Nminute7<=9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                            }
                        }else if(Free_Nsecond7<=9){

                            if (Free_Nsecond7==0&&Free_Nminute7<=30){

                                if (Free_Nminute7>9){
                                    document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                                }else if(Free_Nminute7<=9){
                                    document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;color: #ff1000">'+'0'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                                }

                            }else if (Free_Nminute7>9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+'0'+Free_Nsecond7+':'+Free_Nminute7+'</h1>';
                            }else if(Free_Nminute7<=9){
                                document.getElementById("N_timedown7_free").innerHTML='<h1 id="N_timedown7_free" style="margin-top: 18px;">'+'0'+Free_Nsecond7+':'+'0'+Free_Nminute7+'</h1>';
                            }

                        }


                        // document.getElementById("timedown7_1").innerHTML='<h1 id="timedown7_1" style="margin-top: 10px;">'+second7_+':'+minute7_+'</h1>';
                        Free_Nhandle7 = setTimeout("free_countdown_N7()",1000);
                    }
                    Free_Ntime7--;
                    if (Free_Nminute7>0&&Free_Nminute7<=60){
                        Free_Nminute7--;
                    }else if (Free_Nminute7==0&&Free_Nsecond7!=0){
                        Free_Nminute7 = 59;
                        Free_Nsecond7--;

                    }
                }


                //定义停止函数
                function Free_stoptime7(){

                    Free_Aflag7 = 0;
                    Free_Nflag7 = 0;
                    clearTimeout(Free_Ahandle7);
                    clearTimeout(Free_Nhandle7);

                }
                function Free_Astoptime7(){

                    Free_Aflag7 = 0;
                    clearTimeout(Free_Ahandle7);


                }
                function Free_Nstoptime7(){


                    Free_Nflag7 = 0;
                    clearTimeout(Free_Nhandle7);

                }


            </script>

        


    

