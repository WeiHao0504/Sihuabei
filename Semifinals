        <!-- 计时器数据渲染以及控制-->

                

                    <script>



                        var timeout2_1='180';

                        var second2_1 = parseInt(timeout2_1/60);
                        var minute2_1 = timeout2_1%60;
                        var handle2_1;
                        var flag2_1;

                        timerinit2_1();

                        function timercontroller2_1() {
                            if (flag2_1==0){
                                countdown2_1();
                                flag2_1 = 1;
                            }
                        }


                        function timerinit2_1() {

                            flag2_1 = 0;

                            if (timeout2_1 >=0)
                            {
                                if (second2_1>9){
                                    if (minute2_1>9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+second2_1+':'+minute2_1+'</h1>';
                                    }else if(minute2_1<=9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+second2_1+':'+'0'+minute2_1+'</h1>';
                                    }
                                }else if(second2_1<=9){

                                    if (second2_1==0&&minute2_1<=30){

                                        if (minute2_1>9){
                                            document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+second2_1+':'+minute2_1+'</h1>';
                                        }else if(minute2_1<=9){
                                            document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+second2_1+':'+'0'+minute2_1+'</h1>';
                                        }

                                    }else if (minute2_1>9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+'0'+second2_1+':'+minute2_1+'</h1>';
                                    }else if(minute2_1<=9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+'0'+second2_1+':'+'0'+minute2_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 10px;">'+second2_1+':'+minute2_1+'</h1>';
                            }
                        }

                        function countdown2_1()
                        {
                            // if (timeout2_1 < 0)
                            // {
                            //     document.getElementById("timedown2_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout2_1 >=0)
                            {
                                if (timeout2_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout2_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second2_1>9){
                                    if (minute2_1>9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+second2_1+':'+minute2_1+'</h1>';
                                    }else if(minute2_1<=9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+second2_1+':'+'0'+minute2_1+'</h1>';
                                    }
                                }else if(second2_1<=9){

                                    if (second2_1==0&&minute2_1<=30){

                                        if (minute2_1>9){
                                            document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+second2_1+':'+minute2_1+'</h1>';
                                        }else if(minute2_1<=9){
                                            document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;color: #ff1000">'+'0'+second2_1+':'+'0'+minute2_1+'</h1>';
                                        }

                                    }else if (minute2_1>9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+'0'+second2_1+':'+minute2_1+'</h1>';
                                    }else if(minute2_1<=9){
                                        document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 18px;">'+'0'+second2_1+':'+'0'+minute2_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown2_1").innerHTML='<h1 id="timedown2_1" style="margin-top: 10px;">'+second2_1+':'+minute2_1+'</h1>';
                                handle2_1 = setTimeout("countdown2_1()",1000);
                            }
                            timeout2_1--;
                            if (minute2_1>0&&minute2_1<=60){

                                minute2_1--;
                            }else if (minute2_1==0&&second2_1!=0){


                                minute2_1 = 59;
                                second2_1--;

                            }

                        }
                        // countdown2_1();T

                        function stoptime2_1(){
                            clearTimeout(handle2_1);
                            flag2_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout2_1='180';

                        var N_second2_1 = parseInt(N_timeout2_1/60);
                        var N_minute2_1 = N_timeout2_1%60;
                        var N_handle2_1;
                        var N_flag2_1;

                        N_timerinit2_1();

                        function N_timercontroller2_1() {
                            if (N_flag2_1==0){
                                N_countdown2_1();
                                N_flag2_1 = 1;
                            }
                        }


                        function N_timerinit2_1() {

                            N_flag2_1 = 0;

                            if (N_timeout2_1 >=0)
                            {
                                if (N_second2_1>9){
                                    if (N_minute2_1>9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+N_second2_1+':'+N_minute2_1+'</h1>';
                                    }else if(N_minute2_1<=9){
                                        document.getElementById("N_timedown2_1").innerHTML='<h1 id="N_timedown2_1" style="margin-top: 18px;">'+N_second2_1+':'+'0'+N_minute2_1+'</h1>';
                                    }
                                }else if(N_second2_1<=9){

                                    if (N_second2_1==0&&N_minute2_1<=30){

