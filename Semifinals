        <!-- 计时器数据渲染以及控制-->

                

                    <script>



                        var timeout10_1='300';

                        var second10_1 = parseInt(timeout10_1/60);
                        var minute10_1 = timeout10_1%60;
                        var handle10_1;
                        var flag10_1;

                        timerinit10_1();

                        function timercontroller10_1() {
                            if (flag10_1==0){
                                countdown10_1();
                                flag10_1 = 1;
                            }
                        }


                        function timerinit10_1() {

                            flag10_1 = 0;

                            if (timeout10_1 >=0)
                            {
                                if (second10_1>9){
                                    if (minute10_1>9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+second10_1+':'+minute10_1+'</h1>';
                                    }else if(minute10_1<=9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+second10_1+':'+'0'+minute10_1+'</h1>';
                                    }
                                }else if(second10_1<=9){

                                    if (second10_1==0&&minute10_1<=30){

                                        if (minute10_1>9){
                                            document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+second10_1+':'+minute10_1+'</h1>';
                                        }else if(minute10_1<=9){
                                            document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+second10_1+':'+'0'+minute10_1+'</h1>';
                                        }

                                    }else if (minute10_1>9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+'0'+second10_1+':'+minute10_1+'</h1>';
                                    }else if(minute10_1<=9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+'0'+second10_1+':'+'0'+minute10_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 10px;">'+second10_1+':'+minute10_1+'</h1>';
                            }
                        }

                        function countdown10_1()
                        {
                            // if (timeout10_1 < 0)
                            // {
                            //     document.getElementById("timedown10_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout10_1 >=0)
                            {
                                if (timeout10_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout10_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second10_1>9){
                                    if (minute10_1>9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+second10_1+':'+minute10_1+'</h1>';
                                    }else if(minute10_1<=9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+second10_1+':'+'0'+minute10_1+'</h1>';
                                    }
                                }else if(second10_1<=9){

                                    if (second10_1==0&&minute10_1<=30){

                                        if (minute10_1>9){
                                            document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+second10_1+':'+minute10_1+'</h1>';
                                        }else if(minute10_1<=9){
                                            document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+second10_1+':'+'0'+minute10_1+'</h1>';
                                        }

                                    }else if (minute10_1>9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+'0'+second10_1+':'+minute10_1+'</h1>';
                                    }else if(minute10_1<=9){
                                        document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 18px;">'+'0'+second10_1+':'+'0'+minute10_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 10px;">'+second10_1+':'+minute10_1+'</h1>';
                                handle10_1 = setTimeout("countdown10_1()",1000);
                            }
                            timeout10_1--;
                            if (minute10_1>0&&minute10_1<=60){

                                minute10_1--;
                            }else if (minute10_1==0&&second10_1!=0){


                                minute10_1 = 59;
                                second10_1--;

                            }

                        }
                        // countdown10_1();T

                        function stoptime10_1(){
                            clearTimeout(handle10_1);
                            flag10_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout10_1='0';

                        var N_second10_1 = parseInt(N_timeout10_1/60);
                        var N_minute10_1 = N_timeout10_1%60;
                        var N_handle10_1;
                        var N_flag10_1;

                        N_timerinit10_1();

                        function N_timercontroller10_1() {
                            if (N_flag10_1==0){
                                N_countdown10_1();
                                N_flag10_1 = 1;
                            }
                        }


                        function N_timerinit10_1() {

                            N_flag10_1 = 0;

                            if (N_timeout10_1 >=0)
                            {
                                if (N_second10_1>9){
                                    if (N_minute10_1>9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+N_second10_1+':'+N_minute10_1+'</h1>';
                                    }else if(N_minute10_1<=9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                    }
                                }else if(N_second10_1<=9){

                                    if (N_second10_1==0&&N_minute10_1<=30){

                                        if (N_minute10_1>9){
                                            document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second10_1+':'+N_minute10_1+'</h1>';
                                        }else if(N_minute10_1<=9){
                                            document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                        }

                                    }else if (N_minute10_1>9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+'0'+N_second10_1+':'+N_minute10_1+'</h1>';
                                    }else if(N_minute10_1<=9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+'0'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 10px;">'+second10_1+':'+minute10_1+'</h1>';
                            }
                        }

                        function N_countdown10_1()
                        {
                            // if (timeout10_1 < 0)
                            // {
                            //     document.getElementById("timedown10_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }


                            if (N_timeout10_1 >=0)
                            {

                                if (N_timeout10_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(N_timeout10_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }

                                if (N_second10_1>9){
                                    if (N_minute10_1>9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+N_second10_1+':'+N_minute10_1+'</h1>';
                                    }else if(N_minute10_1<=9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                    }
                                }else if(N_second10_1<=9){

                                    if (N_second10_1==0&&N_minute10_1<=30){

                                        if (N_minute10_1>9){
                                            document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second10_1+':'+N_minute10_1+'</h1>';
                                        }else if(N_minute10_1<=9){
                                            document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                        }

                                    }else if (N_minute10_1>9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+'0'+N_second10_1+':'+N_minute10_1+'</h1>';
                                    }else if(N_minute10_1<=9){
                                        document.getElementById("N_timedown10_1").innerHTML='<h1 id="N_timedown10_1" style="margin-top: 18px;">'+'0'+N_second10_1+':'+'0'+N_minute10_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown10_1").innerHTML='<h1 id="timedown10_1" style="margin-top: 10px;">'+second10_1+':'+minute10_1+'</h1>';
                                N_handle10_1 = setTimeout("N_countdown10_1()",1000);
                            }
                            N_timeout10_1--;
                            if (N_minute10_1>0&&N_minute10_1<=60){
                                N_minute10_1--;
                            }else if (N_minute10_1==0&&N_second10_1!=0){
                                N_minute10_1 = 59;
                                N_second10_1--;

                            }

                        }
                        // countdown10_1();T

                        function N_stoptime10_1(){
                            clearTimeout(N_handle10_1);
                            N_flag10_1 = 0;
                        }

                    </script>

                









        


    




