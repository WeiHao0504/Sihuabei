                    <script>



                        var timeout3_1='180';

                        var second3_1 = parseInt(timeout3_1/60);
                        var minute3_1 = timeout3_1%60;
                        var handle3_1;
                        var flag3_1;

                        timerinit3_1();

                        function timercontroller3_1() {
                            if (flag3_1==0){
                                countdown3_1();
                                flag3_1 = 1;
                            }
                        }


                        function timerinit3_1() {

                            flag3_1 = 0;

                            if (timeout3_1 >=0)
                            {
                                if (second3_1>9){
                                    if (minute3_1>9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+second3_1+':'+minute3_1+'</h1>';
                                    }else if(minute3_1<=9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+second3_1+':'+'0'+minute3_1+'</h1>';
                                    }
                                }else if(second3_1<=9){

                                    if (second3_1==0&&minute3_1<=30){

                                        if (minute3_1>9){
                                            document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+second3_1+':'+minute3_1+'</h1>';
                                        }else if(minute3_1<=9){
                                            document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+second3_1+':'+'0'+minute3_1+'</h1>';
                                        }

                                    }else if (minute3_1>9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+'0'+second3_1+':'+minute3_1+'</h1>';
                                    }else if(minute3_1<=9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+'0'+second3_1+':'+'0'+minute3_1+'</h1>';
                                    }

                                }

                                // document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 10px;">'+second3_1+':'+minute3_1+'</h1>';
                            }
                        }

                        function countdown3_1()
                        {
                            // if (timeout3_1 < 0)
                            // {
                            //     document.getElementById("timedown3_1").value = '时间到';
                            //     if(document.getElementById("downloadfile"))document.getElementById("downloadfile").style.display = '';
                            // }
                            if (timeout3_1 >=0)
                            {
                                if (timeout3_1 ==0){
                                    $("#jquery_jplayer_2").jPlayer("play");
                                }
                                if(timeout3_1 ==30){
                                    $("#jquery_jplayer_1").jPlayer("play");
                                }
                                if (second3_1>9){
                                    if (minute3_1>9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+second3_1+':'+minute3_1+'</h1>';
                                    }else if(minute3_1<=9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+second3_1+':'+'0'+minute3_1+'</h1>';
                                    }
                                }else if(second3_1<=9){

                                    if (second3_1==0&&minute3_1<=30){

                                        if (minute3_1>9){
                                            document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+second3_1+':'+minute3_1+'</h1>';
                                        }else if(minute3_1<=9){
                                            document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;color: #ff1000">'+'0'+second3_1+':'+'0'+minute3_1+'</h1>';
                                        }

                                    }else if (minute3_1>9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+'0'+second3_1+':'+minute3_1+'</h1>';
                                    }else if(minute3_1<=9){
                                        document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 18px;">'+'0'+second3_1+':'+'0'+minute3_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown3_1").innerHTML='<h1 id="timedown3_1" style="margin-top: 10px;">'+second3_1+':'+minute3_1+'</h1>';
                                handle3_1 = setTimeout("countdown3_1()",1000);
                            }
                            timeout3_1--;
                            if (minute3_1>0&&minute3_1<=60){

                                minute3_1--;
                            }else if (minute3_1==0&&second3_1!=0){


                                minute3_1 = 59;
                                second3_1--;

                            }

                        }
                        // countdown3_1();T

                        function stoptime3_1(){
                            clearTimeout(handle3_1);
                            flag3_1 = 0;
                        }

                    </script>

                    <script>
                        var N_timeout3_1='180';

                        var N_second3_1 = parseInt(N_timeout3_1/60);
