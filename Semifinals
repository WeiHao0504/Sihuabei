                                    if (N_minute5_1>9){
                                        document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;">'+N_second5_1+':'+N_minute5_1+'</h1>';
                                    }else if(N_minute5_1<=9){
                                        document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;">'+N_second5_1+':'+'0'+N_minute5_1+'</h1>';
                                    }
                                }else if(N_second5_1<=9){

                                    if (N_second5_1==0&&N_minute5_1<=30){

                                        if (N_minute5_1>9){
                                            document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second5_1+':'+N_minute5_1+'</h1>';
                                        }else if(N_minute5_1<=9){
                                            document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;color: #ff1000">'+'0'+N_second5_1+':'+'0'+N_minute5_1+'</h1>';
                                        }

                                    }else if (N_minute5_1>9){
                                        document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;">'+'0'+N_second5_1+':'+N_minute5_1+'</h1>';
                                    }else if(N_minute5_1<=9){
                                        document.getElementById("N_timedown5_1").innerHTML='<h1 id="N_timedown5_1" style="margin-top: 18px;">'+'0'+N_second5_1+':'+'0'+N_minute5_1+'</h1>';
                                    }

                                }


                                // document.getElementById("timedown5_1").innerHTML='<h1 id="timedown5_1" style="margin-top: 10px;">'+second5_1+':'+minute5_1+'</h1>';
                                N_handle5_1 = setTimeout("N_countdown5_1()",1000);
                            }
                            N_timeout5_1--;
                            if (N_minute5_1>0&&N_minute5_1<=60){
                                N_minute5_1--;
                            }else if (N_minute5_1==0&&N_second5_1!=0){
                                N_minute5_1 = 59;
                                N_second5_1--;

                            }

                        }
                        // countdown5_1();T

                        function N_stoptime5_1(){
                            clearTimeout(N_handle5_1);
                            N_flag5_1 = 0;
                        }

                    </script>

                









        


    

